{
  "name": "@adobe/commerce-cif-commercetools-category",
  "version": "0.1.10",
  "description": "Adobe Commerce Integration Framework (CIF) Commercetools Integration Category Project",
  "author": "Adobe Systems Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/commerce-cif-commercetools.git"
  },
  "dependencies": {
    "@adobe/commerce-cif-commercetools-common": "0.1.19",
    "@adobe/commerce-cif-common": "0.1.17",
    "@adobe/commerce-cif-model": "0.1.123",
    "@commercetools/sdk-client": "1.4.1"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-plugin-header": "1.2.0",
    "js-yaml": "3.9.0",
    "serverless": "1.27.3",
    "serverless-openwhisk": "0.13.0",
    "serverless-webpack": "4.3.0",
    "webpack": "4.0.1"
  },
  "eslintConfig": {
    "extends": "../../.eslintrc.js"
  },
  "scripts": {
    "lint": "$(npm bin)/eslint -f table --ignore-path ../../.eslintignore .",
    "deploy": "$(npm bin)/serverless deploy",
    "remove": "$(npm bin)/serverless remove",
    "deploy-current": "npm run deploy -- --ow-package commerce-cif-commercetools-category@$npm_package_version --shared",
    "deploy-latest": "npm run deploy -- --ow-package commerce-cif-commercetools-category@latest --shared",
    "deploy-suffix": "npm run deploy -- --ow-package commerce-cif-commercetools-category@${OW_PACKAGE_SUFFIX} --shared",
    "deploy-package": "npm run deploy-current; npm run deploy-latest",
    "remove-current": "npm run remove -- --ow-package commerce-cif-commercetools-category@$npm_package_version",
    "remove-latest": "npm run remove -- --ow-package commerce-cif-commercetools-category@latest",
    "remove-suffix": "npm run remove -- --ow-package commerce-cif-commercetools-category@${OW_PACKAGE_SUFFIX}",
    "remove-package": "npm run remove-current; npm run remove-latest"
  }
}
